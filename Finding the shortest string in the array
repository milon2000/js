function findShort(s){
var splitArray = s.split(" ");
var shortLength = Infinity;
var shortest ="";
for (var i = 0; i < splitArray.length; i++) {
 if (splitArray[i].length < shortLength) {
  shortest = splitArray[i];
  shortLength = splitArray[i].length;
 }
}
return shortest.length;
}

findShort("The quick brown fox jumped over the lazy dog");
